], key[12:16]]\n","\n","    print(\"Ключи раунда: ------------------\")\n","    for i, rk in enumerate(round_keys):\n","        print(f\"k{i}=\", to_binary_string(rk))\n","\n","    print(\"\\nРаунд 0 ------------------\")\n","    print(\"Добавление ключа =\", to_binary_string(input_text))\n","    y0 = saes_round(input_text, round_keys[0])\n","    print(\"Y0=\", to_binary_string(y0))\n","\n","    print(\"\\nРаунд 1 ------------------\")\n","    y1 = saes_round(y0, round_keys[1])\n","    print(\"Y1=\", to_binary_string(y1))\n","\n","    print(\"\\nРаунд 2 ------------------\")\n","    y2 = saes_round(y1, round_keys[2])\n","    print(\"Y2=\", to_binary_string(y2))\n","\n","# Пример использования\n","input_text = [1, 1, 1, 1, 0, 0, 1, 0, 0, 0, 0, 1, 1, 1, 0, 1]\n","key = [1, 1, 1, 0, 1, 0, 0, 0, 1, 1, 1, 1, 0, 1, 0, 1]\n","\n","saes(input_text, key)\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":511},"id":"ZhCs1nneq0Qn","executionInfo":{"status":"error","timestamp":1702246107775,"user_tz":-180,"elapsed":456,"user":{"displayName":"Михаил Ильин","userId":"04782454965296287833"}},"outputId":"271a0a91-d136-4265-f194-334a1ac6d5c8"},"execution_count":26,"outputs":[{"output_type":"stream","name":"stdout","text":["Ключи раунда: ------------------\n","k0= 00000001 00000001 00000001 00000000\n","k1= 00000001 00000000 00000000 00000000\n","k2= 00000001 00000001 00000001 00000001\n","k3= 00000000 00000001 00000000 00000001\n","\n","Раунд 0 ------------------\n","Добавление ключа = 00000001 00000001 00000001 00000001 00000000 00000000 00000001 00000000 00000000 00000000 00000000 00000001 00000001 00000001 00000000 00000001\n"]},{"output_type":"error","ename":"IndexError","evalue":"ignored","traceback":["\u001b[0;31m---------------------------------------------------------------------------\u001b[0m","\u001b[0;31mIndexError\u001b[0m                                Traceback (most recent call last)","\u001b[0;32m<ipython-input-26-660c3ed8aa8e>\u001b[0m in \u001b